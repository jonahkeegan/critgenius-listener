{
  "name": "@critgenius/client",
  "version": "0.1.0",
  "description": "CritGenius Listener React Client - Real-time audio capture interface",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:unit": "vitest run --reporter=verbose",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist .turbo node_modules/.cache"
  },
  "dependencies": {
    "react": "catalog:",
    "react-dom": "catalog:",
    "@critgenius/shared": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "@testing-library/react": "catalog:",
    "@testing-library/jest-dom": "catalog:",
    "jsdom": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:",
    "rimraf": "catalog:"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}