{
  "name": "@critgenius/listener",
  "version": "0.1.0",
  "description": "CritGenius Listener - Real-time audio capture and processing for intelligent feedback analysis",
  "type": "module",
  "private": true,
  "keywords": [
    "audio",
    "real-time",
    "feedback",
    "analysis",
    "typescript",
    "react",
    "express"
  ],
  "author": {
    "name": "Jonah Keegan",
    "email": "jonah@clicktruemedia.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonahkeegan/critgenius-listener.git"
  },
  "homepage": "https://github.com/jonahkeegan/critgenius-listener#readme",
  "bugs": {
    "url": "https://github.com/jonahkeegan/critgenius-listener/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@9.15.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"pnpm --filter @critgenius/server dev\" \"pnpm --filter @critgenius/client dev\"",
    "build": "pnpm --filter @critgenius/shared build && pnpm --filter @critgenius/server build && pnpm --filter @critgenius/client build",
    "start": "pnpm --filter @critgenius/server start",
    "clean": "pnpm --recursive exec rimraf dist build .turbo node_modules/.cache",
    "clean:all": "pnpm clean && pnpm --recursive exec rimraf node_modules && rimraf node_modules",
    "test": "pnpm --recursive test",
    "test:unit": "pnpm --recursive test:unit",
    "test:integration": "pnpm --recursive test:integration",
    "test:watch": "pnpm --recursive test:watch",
    "test:coverage": "pnpm --recursive test:coverage",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "type-check": "pnpm --recursive type-check",
    "install:clean": "pnpm clean:all && pnpm install",
    "prepare": "husky install",
    "precommit": "pnpm lint:fix && pnpm format && pnpm type-check"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "concurrently": "catalog:",
    "eslint": "catalog:",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "husky": "^9.1.6",
    "prettier": "catalog:",
    "rimraf": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "^8.15.0"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "catalog:",
      "@types/react-dom": "catalog:"
    }
  }
}